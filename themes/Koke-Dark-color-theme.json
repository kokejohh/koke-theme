	"tokenColors": [
		{
			"name": "Text",
			"scope": [
				"text"
			],
			"settings": {
				"foreground": "#eeeeee",
			}
		},
		{
			"name": "Source",
			"scope": [
				"source"
			],
			"settings": {
				"foreground": "#eeeeee",
			}
		},
		{
			"name": "Punctuation String",
			"scope": [
				"punctuation.definition.string"
			],
			"settings": {
				"foreground": "#ecea62",
			}
		},
		{
			"name": "Punctuation Terminator",
			"scope": [
				"punctuation.terminator"
			],
			"settings": {
				"foreground": "#eeeeee",
			}
